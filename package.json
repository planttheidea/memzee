{
  "author": "tony.quetano@planttheidea.com",
  "bugs": {
    "url": "https://github.com/planttheidea/memzee/issues"
  },
  "description": "Function memoization based on only the most recent arguments",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "benchee": "^1.0.0",
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.2",
    "cli-table2": "^0.2.0",
    "fast-copy": "^1.2.3",
    "fast-equals": "^1.6.1",
    "fast-memoize": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "lru-memoize": "^1.0.2",
    "mem": "^4.0.0",
    "memoize-one": "^4.0.2",
    "memoizee": "^0.4.14",
    "memoizerific": "^1.11.3",
    "mini-bench": "^1.0.0",
    "moize": "^5.4.1",
    "ora": "^3.0.0",
    "performance-now": "^2.1.0",
    "ramda": "^0.25.0",
    "rollup": "^0.66.6",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rsvp": "^4.8.4",
    "simple-statistics": "^6.1.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.3",
    "underscore": "^1.9.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "homepage": "https://github.com/planttheidea/memzee#readme",
  "keywords": [
    "memoize",
    "memoization",
    "cache",
    "moize",
    "mem",
    "memcache"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "memzee",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/memzee.git"
  },
  "scripts": {
    "benchmark": "npm run dist && NODE_ENV=production node ./benchmarks/index.js",
    "build": "NODE_ENV=production rollup -c",
    "build:types": "cp src/types.ts dist/index.d.ts",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development webpack-dev-server --colors --progress --config=webpack/webpack.config.js",
    "dist": "npm run clean && npm run build && npm run build:types",
    "lint": "NODE_ENV=test tslint 'src/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "if in-publish; then npm run prepublish:compile; fi",
    "prepublish:compile": "npm run lint && npm run test:coverage && npm run dist",
    "start": "npm run dev",
    "test": "NODE_PATH=. jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "version": "1.0.0"
}
